<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>QLSVLP5</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>SinhVienServlet</servlet-name>
    <servlet-class>controller.SinhVienServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SinhVienServlet</servlet-name>
    <url-pattern>/SinhVienServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SinhVienServlet</servlet-name>
    <url-pattern></url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AddSinhVienServlet</display-name>
    <servlet-name>AddSinhVienServlet</servlet-name>
    <servlet-class>controller.AddSinhVienServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddSinhVienServlet</servlet-name>
    <url-pattern>/AddSinhVienServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>EditSinhVienServlet</display-name>
    <servlet-name>EditSinhVienServlet</servlet-name>
    <servlet-class>controller.EditSinhVienServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EditSinhVienServlet</servlet-name>
    <url-pattern>/EditSinhVienServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DeleteSinhVienServlet</display-name>
    <servlet-name>DeleteSinhVienServlet</servlet-name>
    <servlet-class>controller.DeleteSinhVienServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteSinhVienServlet</servlet-name>
    <url-pattern>/DeleteSinhVienServlet</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>ChangeLanguageFilter</display-name>
    <filter-name>ChangeLanguageFilter</filter-name>
    <filter-class>controller.filter.ChangeLanguageFilter</filter-class>
  </filter>
  <filter>
    <display-name>testFiler</display-name>
    <filter-name>testFiler</filter-name>
    <filter-class>controller.filter.testFiler</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>testFiler</filter-name>
    <url-pattern>/testFiler</url-pattern>
  </filter-mapping>
  <filter>
    <display-name>LoginFilter</display-name>
    <filter-name>LoginFilter</filter-name>
    <filter-class>controller.filter.LoginFilter</filter-class>
  </filter>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>controller.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>60</session-timeout>
  </session-config>
  <filter>
    <filter-name>encodeText</filter-name>
    <filter-class>org.apache.catalina.filters.SetCharacterEncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>encodeText</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>ChangeLanguageFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>LoginFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>LogoutServlet</display-name>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>controller.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Faces Servlet</servlet-name>
    <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>Faces Servlet</servlet-name>
    <url-pattern>/faces/*</url-pattern>
  </servlet-mapping>
</web-app>