<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:mvc="http://www.springframework.org/schema/mvc"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc.xsd
http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="thayThanh"
		class="giangvien.GiangVienThanh"></bean>
	<bean id="thayThang"
		class="giangvien.GiangVienThang"></bean>
	<bean id="thayLam" class="giangvien.GiangVienLam"></bean>
	<bean id="monHocLapTrinhCoBan"
		class="monhoc.MonHocLapTrinhCoBan"></bean>
	<bean id="monHocLapTrinhJavaCoBan"
		class="monhoc.MonHocLapTrinhJavaCoBan"></bean>
	<bean id="monHocLapTrinhJavaWeb"
		class="monhoc.MonHocLapTrinhJavaWeb"></bean>
	<bean id="monHocLapTrinhWeb"
		class="monhoc.MonHocLapTrinhWeb"></bean>
	<bean id="monHocLapTrinhWebPHP"
		class="monhoc.MonHocLapTrinhWebPHP"></bean>
	<bean id="monHocQuyTrinhDuAn"
		class="monhoc.MonHocQuyTrinhDuAn"></bean>
	<bean id="monHocTinHocVanPhong"
		class="monhoc.MonHocTinHocVanPhong"></bean>
	<bean id="loTrinhHocOffline"
		class="lotrinhhoc.LoTrinhHocOffline"></bean>
	<bean id="loTrinhHocOnline"
		class="lotrinhhoc.LoTrinhHocOnline"></bean>
	<bean id="heDaoTaoDaiHan"
		class="hedaotao.HeDaoTaoDaiHan">
		<property name="danhsachmonhoc">
			<list>
				<ref bean="monHocLapTrinhCoBan" />
				<ref bean="monHocLapTrinhJavaCoBan" />
				<ref bean="monHocLapTrinhJavaWeb" />
				<ref bean="monHocLapTrinhWeb" />
				<ref bean="monHocLapTrinhWebPHP" />
				<ref bean="monHocQuyTrinhDuAn" />
				<ref bean="monHocTinHocVanPhong" />
			</list>
		</property>
	</bean>
	<bean id="heDaoTaoNganHanPHP"
		class="hedaotao.HeDaoTaoNganHanPHP">
		<property name="danhsachmonhoc">
		<list>
			<ref bean="monHocLapTrinhCoBan" />
			<ref bean="monHocLapTrinhWeb" />
			<ref bean="monHocLapTrinhWebPHP" />
		</list>
		</property>
	</bean>
	<bean id="heDaoTaoNganHanJava"
		class="hedaotao.HeDaoTaoNganHanJava">
		<property name="danhsachmonhoc">
		<list>
			<ref bean="monHocLapTrinhJavaCoBan" />
			<ref bean="monHocLapTrinhJavaWeb" />
			<ref bean="monHocLapTrinhWeb" />
		</list>
		</property>
	</bean>

	<bean id="sv1" class="hocsinh.HocSinh"
		scope="prototype">
		<property name="hoTen" value="Nguyen Manh Dung"></property>
		<property name="diaChi" value="Da Nang"></property>
		<property name="namSinh" value="1999"></property>
		<property name="heDaoTao" ref="heDaoTaoDaiHan"></property>
		<property name="loTrinhHoc" ref="loTrinhHocOffline"></property>
		<property name="dienThoai" value="01256487956"></property>
		<property name="email" value="dung@gmail.com"></property>
	</bean>
</beans>